#!/bin/bash

# PROCESS FLOW TWEAKER DURING ALL BUT CHROOT STAGE. DO NOT EDIT THIS FILE!
case "$1" in
	1)
		sed -i -e 's/KEY=0/KEY=1/g' "${CLOCALLG}/sinprog"
		sed -i -e 's/NEW=0/NEW=1/g' "${CLOCALLG}/sinprog";;
	2)
		sed -i -e 's/WGETST3=0/WGETST3=1/g' "${CLOCALLG}/sinprog";;
	3)
		sed -i -e 's/VERIFICATION=0/VERIFICATION=1/g' "${CLOCALLG}/sinprog";;
	4)
		sed -i -e 's/EXTRACTION=0/EXTRACTION=1/g' "${CLOCALLG}/sinprog"
		sed -i -e 's/PARTA=0/PARTA=1/g' "${CLOCALLG}/sinprog";;
	5)
		sed -i -e 's/1/0/g' "${CLOCALLG}/sinprog"
		sed -i -e 's/NEW=1/NEW=0/g' "${CLOCALLG}/sinprog"
		sed -i -e 's/PARTA=1/PARTA=0/g' "${CLOCALLG}/sinprog";;
	6)
		sed -i -e 's/PARTB=0/PARTA=1/g' "${CLOCALLG}/sinprog";;
	catalyst)
		sed -i -e 's/CATALYST=0/CATALYST=1/g' "${CLOCALLG}/sinprog";;
	precomp)
		sed -i -e 's/PRECOMP=0/PRECOMP=1/g' "${CLOCALLG}/sinprog";;
	localrb)
		sed -i -e 's/LOCALRB=0/LOCALRB=1/g' "${CLOCALLG}/sinprog";;
	dn)
		;;
esac
